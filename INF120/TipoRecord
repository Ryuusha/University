program Work1;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;

type Persona=Record
  CI:integer;
  nombre:string;
  telefono:integer;
  procedencia:string;
End;
var p:Persona; i:integer;

begin
  writeln('CI: ');
  readln(p.CI); writeln('procedencia: ');  readln(p.procedencia);
  writeln('nombre: ');
  readln(p.nombre);
  writeln('telefono: ');
  readln(p.telefono);

  writeln('CI: ',p.CI,' ',p.procedencia);
  writeln('nombre: ',p.nombre,' ','telefono: ',p.telefono);
  readln;

end.
